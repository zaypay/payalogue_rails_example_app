<!DOCTYPE html>
<html>
  <head>
    <title>PayalogueExample</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield :head %>
  </head>
  <body>
    <div id="container">
      <div id="table_unit">
        <% if flash[:error] %>
          <div class="alert alert-error">
            <%= flash[:error] %>
          </div>
        <% end %>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
